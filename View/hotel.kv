        name: 'out'
    CheckIn:
        name: 'in'
    CadastrarFuncionario:
        name: 'cadastroFuncioonario'

<LabelPreto@Label>:
    color: (0,0,0,1)

<actionMenu@ActionBar>:
    ActionBar:
        ActionView:
            use_separator: True
            ActionPrevious:
                title:'Menu'
                on_release: app.root.current = 'menu'
            ActionButton:
                text: 'Cadastro'
                on_release: app.root.current = 'cadastro'
            ActionButton:
                text: 'Clientes'
                on_release: app.root.current = 'clientes'
            ActionButton:
                text: 'Senhas'
                on_release: app.root.current = 'senha'
            ActionButton:
                text: 'Quartos'
                on_release: app.root.current = 'quarto'
            ActionButton:
                text: 'Pedidos'
                on_release: app.root.current = 'pedido'
            ActionButton:
                text: 'Check-out'
                on_release: app.root.current = 'out'
            ActionButton:
