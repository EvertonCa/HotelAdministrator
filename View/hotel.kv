                    BoxLayout:
                        size_hint_y: None
                        height: 30
                        LabelPreto:
                            text: 'Numero do quarto:'
                        TextInput:
                            id: numQuarto
                            multiline: False

                    BoxLayout:
                        size_hint_y: None
                        height: 60
                        Button:
                            text: 'OK'
                            on_release: root.addPessQuarto(numPessIn.text, numQuarto.text)


