                        spacing: 20
                        size_hint_y: None
                        height: 60

                        LabelPreto:
                            text: 'Quantidade de pessoas no quarto:'

                        Spinner:
                            id: numPessIn
                            text: "0"
                            values: ["1", "2", "3", "4", "5"]
                            on_text: root.qtdPessoas(numPessIn.text)

                    BoxLayout:
                        id: inputNomes
                        size_hint_y: None
