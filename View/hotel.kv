<Gerenciador>:
    LoginLayout:
        name: 'login'
    Menu:
        name: 'menu'
    Cadastrar:
        name: 'cadastro'
    Cliente:
        name: 'clientes'
    Senha:
        name: 'senha'
    Quartos:
        name: 'quarto'
    Pedidos:
        name: 'pedido'
    CheckOut:
        name: 'out'
    CheckIn:
        name: 'in'
    CadastrarFuncionario:
        name: 'cadastroFuncioonario'

<LabelPreto@Label>:
    color: (0,0,0,1)



<LoginLayout>:
    BoxLayout:
        orientation:'vertical'

        ScrollView:
            BoxLayout:
                orientation: 'vertical'
                size_hint_y: None
                height: self.minimum_height
                spacing: 20
                padding: 100

                Image:
                    source: 'Images/logo-hotel.png'
                    size_hint_y: None
                    height: 200
                    allow_stretch: True


                BoxLayout:
                    size_hint_y: None
                    height: 30
                    LabelPreto:
                        text: 'Login:'
                    TextInput:
                        id: loginText
                        font_size: 16
                        multiline: False
                BoxLayout:
                    size_hint_y: None
                    height: 30
                    LabelPreto:
                        text: 'Senha:'
                    TextInput:
                        id: passwordText
                        multiline: False
                        password: True

                BoxLayout:
                    size_hint_y: None
                    height: 60
                    Button:
                        font_size: 16
                        text: 'Entrar'
                        on_press: root.verificaLogin(loginText.text,passwordText.text)


<Menu>:
    BoxLayout:
        orientation:'vertical'

        ActionBar:
            ActionView:
                use_separator: True
                ActionOverflow:
                    ActionButton:
                        text: 'Logout'
                        on_release: app.root.current = 'login'
                    ActionButton:
                        text: 'Fechar'
                        on_release: app.stop()
                ActionPrevious:
                    title: 'Bem Vindo!'
        BoxLayout:
            orientation:'vertical'
            padding: 60
            spacing: 30


            BoxLayout:
                spacing: 30
                Button:
                    text: 'Cadastrar Cliente'
                    on_release: app.root.current = 'cadastro'
                Button:
                    text: 'Check-in'
                    on_release: app.root.current = 'in'
                Button:
                    text: 'Check-out'
                    on_release: app.root.current = 'out'

            BoxLayout:
                spacing: 30
                Button:
                    text: 'Quartos'
                    on_release: app.root.current = 'quarto'
                Button:
                    text: 'Pedidos'
                    on_release: app.root.current = 'pedido'
                Button:
                    text: 'Cliente'
                    on_release: app.root.current = 'clientes'

            BoxLayout:
                spacing: 30
                Button:
                    text: 'Cadastra Funcionario'
                    on_release: app.root.current = 'cadastroFuncioonario'
                Button:
                    text: 'Senhas'
                    on_release: app.root.current = 'senha'

<Cadastrar>:
    BoxLayout:
        orientation: 'vertical'
        ActionBar:
            ActionView:
                use_separator: True
                ActionPrevious:
                    title:'Menu'
                    on_release: app.root.current = 'menu'
                ActionButton:
                    text: 'Cadastro'
                    on_release: app.root.current = 'cadastro'
                ActionButton:
                    text: 'Cliente'
                    on_release: app.root.current = 'clientes'
                ActionButton:
                    text: 'Senhas'
                    on_release: app.root.current = 'senha'
                ActionButton:
                    text: 'Quartos'
                    on_release: app.root.current = 'quarto'
                ActionButton:
                    text: 'Pedidos'
                    on_release: app.root.current = 'pedido'
                ActionButton:
                    text: 'Check-out'
                    on_release: app.root.current = 'out'
                ActionButton:
                    text: 'Check-in'
                    on_release: app.root.current = 'in'
                ActionButton:
                    text: 'Sair'
                    on_release: app.stop()
        BoxLayout:
            orientation: 'vertical'

            ScrollView:
                BoxLayout:
                    orientation: 'vertical'
                    size_hint_y: None
                    height: self.minimum_height
                    padding: 100
                    spacing: 20

                    BoxLayout:
                        size_hint_y: None
                        height: 40
                        LabelPreto:
                            text: 'Nome:'
                        TextInput:
                            id: nome
                            font_size: 16
                            multiline: False

                    BoxLayout:
                        size_hint_y: None
                        height: 40
                        LabelPreto:
                            text: 'Telefone:'
                        TextInput:
                            id: telefone
                            font_size: 16
                            multiline: False

                    BoxLayout:
                        size_hint_y: None
                        height: 40
                        LabelPreto:
                            text: 'CPF:'
                        TextInput:
                            id: cpf
                            font_size: 16
                            multiline: False

                    BoxLayout:
                        size_hint_y: None
                        height: 40
                        LabelPreto:
                            text: 'Endereco:'
                        TextInput:
                            id: endeteco
                            font_size: 16

                    BoxLayout:
                        size_hint_y: None
                        height: 40
                        BoxLayout:
                            LabelPreto:
                                text: 'Sexo:'
                        BoxLayout:
                            CheckBox:
                                color: 0, 0, 0, 1
                                group: "sexo_pessoa"
                                on_active: root.homem(*args)
                            LabelPreto:
                                text: 'M'
                            CheckBox:
                                color: 0, 0, 0, 1
                                group: "sexo_pessoa"
                                on_active: root.mulher(*args)
                            LabelPreto:
                                text: 'F'


                    BoxLayout:
                        size_hint_y: None
                        height: 60
                        Button:
                            font_size: 16
                            text: 'OK'
                            on_release: root.cadastraPessoa(nome.text, telefone.text, cpf.text, endeteco.text)


