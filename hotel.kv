<CustButton@Button>:
    font_size: 32

<RetiraSenhaLayout>:
    id: atendente
    display: suaSenha
    rows: 5
    padding: 10
    spacing: 10

    BoxLayout:
        spacing: 10
        CustButton:
            text: "Senha"
            on_release: atendente.novaSenha(suaSenha.text, False)
        CustButton:
            text: "Senha preferencial"
            on_release: atendente.novaSenha(suaSenha.text, True)
    BoxLayout:
        orientation: 'horizontal'
        Label:
            text: 'Sua senha eh:'
            font_size: 30
        Label:
            id: suaSenha
            text: '0'
            font_size: 30

<Gerenciador>:
    LoginLayout:
        name: 'login'
    Menu:
        name: 'menu'
    Tarefas:
        name: 'tarefas'
    Cadastrar:
        name: 'cadastro'
    Buscar:
        name: 'buscar'
    Senhas:
        name: 'senha'
    Quartos:
        name: 'quarto'
    Pedidos:
        name: 'pedido'
    CheckOut:
        name: 'out'
    CheckIn:
        name: 'in'

<LabelPreto@Label>:
    color: (0,0,0,1)

<actionMenu@ActionBar>:
    ActionBar:
        ActionView:
            use_separator: True
            ActionPrevious:
                title:'Menu'
                on_release: app.root.current = 'menu'
            ActionButton:
                text: 'Cadastro'
                on_release: app.root.current = 'cadastro'
            ActionButton:
                text: 'Buscar'
                on_release: app.root.current = 'buscar'
            ActionButton:
                text: 'Senhas'
                on_release: app.root.current = 'senha'
            ActionButton:
                text: 'Quartos'
                on_release: app.root.current = 'quarto'
            ActionButton:
                text: 'Pedidos'
                on_release: app.root.current = 'pedido'
            ActionButton:
                text: 'Check-out'
                on_release: app.root.current = 'out'
            ActionButton:
                text: 'Check-in'
                on_release: app.root.current = 'in'
            ActionButton:
                text: 'Sair'
                on_release: app.stop()


<LoginLayout>:
    BoxLayout:
        id: login
        padding: 200
        spacing: 10
        orientation: 'vertical'

        BoxLayout:
            Image:
                source: 'logo-hotel.png'
                size_hint_y: None
                height: 200
                allow_stretch: True
        BoxLayout:
            size_hint_y: None
            height: 30
            LabelPreto:
                text: 'Login:'
            TextInput:
                id: loginText
                font_size: 16
                multiline: False
        BoxLayout:
            size_hint_y: None
            height: 30
            LabelPreto:
                text: 'Senha:'
            TextInput:
                id: passwordText
                multiline: False
                password: True

        Button:
            size_hint_y: None
            height: 60
            font_size: 16
            text: 'Entrar'
            on_press: root.verificaLogin(loginText.text,passwordText.text)


<Menu>:
    BoxLayout:
        orientation:'vertical'
        padding: 150
        BoxLayout:
            Button:
                text: 'Cadastrar'
                on_release: app.root.current = 'cadastro'
            Button:
                text: 'Buscar'
                on_release: app.root.current = 'buscar'
            Button:
